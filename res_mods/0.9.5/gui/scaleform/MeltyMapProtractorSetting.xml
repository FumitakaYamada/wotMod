    <enableEffArmor>true</enableEffArmor>    <!-- Displays effective armor calculation. ** only when armor values are present in "vehiclesData" section. -->
    <enableRulerWhenTD>false</enableRulerWhenTD>    <!-- Displays GunArc Critical Angle Ruler for TD's. -->
    <enableRulerWhenSPG>false</enableRulerWhenSPG>    <!-- Displays GunArc Critical Angle Ruler for SPG's. -->
    <minimapEx>false</minimapEx>    <!-- Enables MiniMap Enhancement. -->
    <damagePanelEx>false</damagePanelEx>  <!-- Enables Damage Panel Enhancement. (Displays time to repair). -->
